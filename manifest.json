{
  "manifest_version": 2,
  "name": "Open Shortcut",
  "version": "1.0",
  "description": "A simple Firefox extension to open Shortcut.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click to open Shortcut"
  }
}
